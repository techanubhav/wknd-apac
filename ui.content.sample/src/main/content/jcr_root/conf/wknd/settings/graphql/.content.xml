<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured">
    <persistentQueries jcr:primaryType="nt:unstructured">
        <adventures-all
            jcr:primaryType="nt:unstructured"
            sling:resourceType="graphql/persistent/query">
            <jcr:content
                jcr:data="{Binary}"
                jcr:mimeType="text/html"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="graphql/persistent/query/content"/>
        </adventures-all>
        <plain-article-query
            jcr:created="{Date}2021-05-31T02:09:49.717Z"
            jcr:primaryType="nt:unstructured"
            sling:resourceType="graphql/persistent/query">
            <jcr:content
                cq:lastReplicated="{Date}2021-05-31T02:18:05.798Z"
                cq:lastReplicatedBy="nikolovs@adobe.com"
                cq:lastReplicationAction="Activate"
                jcr:data="{Binary}"
                jcr:mimeType="text/html"
                jcr:mixinTypes="[cq:ReplicationStatus]"
                jcr:primaryType="nt:unstructured"
                sling:resourceType="graphql/persistent/query/content"/>
        </plain-article-query>
    </persistentQueries>
</jcr:root>
